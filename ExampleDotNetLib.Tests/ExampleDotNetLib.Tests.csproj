<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TargetFrameworkVersion>v4.8</TargetFrameworkVersion>
    <OutputType>Library</OutputType>
    <IsPackable>false</IsPackable>
    <TestOutputPath>$(MSBuildProjectDirectory)\TestOutput</TestOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NUnit" Version="3.13.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ExampleDotNetLib\ExampleDotNetLib.csproj" />
  </ItemGroup>

  <Target Name="BuildTarget">
    <MSBuild Projects="$(MSBuildProjectFile)" Targets="CoreBuild" />
  </Target>

  <Target Name="CoreBuild" />

  <Target Name="BeforeBuild">
      <PropertyGroup>
          <OutputPath>$(TestOutputPath)</OutputPath>
      </PropertyGroup>
  </Target>
</Project>
